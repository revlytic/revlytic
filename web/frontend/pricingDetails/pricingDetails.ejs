    <script>
      function submitForm() {
        // Fetch form elements
        var name = document.getElementById("name").value;
        var email = document.getElementById("email").value;
        var message = document.getElementById("message").value;
        var frm = document.getElementsByName("contact-form")[0];

        // You can perform additional validation here if needed

        // Example: Check if fields are not empty
        if (
          name.trim() === "" ||
          email.trim() === "" ||
          message.trim() === ""
        ) {
          alert("Please fill in all required fields");
          return;
        }
        let emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
        if (!emailRegex.test(email)) {
          alert("Please enter a valid email address");
          return;
        }

        // Show loading spinner and disable the button
        document.getElementById("loader").style.display = "inline-block";
        document.getElementById("buttonText").style.display = "none";

        // Perform your desired actions with the form data
        // For now, let's just log the data to the console
        console.log("Name: " + name);
        console.log("Email: " + email);
        console.log("Message: " + message);

        // Construct the data to be sent to the server
        var formData = {
          name: name,
          email: email,
          message: message,
        };

        // Use the fetch API to send the data to the server
        fetch(
          "https://revlytic.co/api/emailSendFromShopifyAppStoreContactPopUp",
          {
            method: "POST",
            headers: {
              "Content-Type": "application/json",
            },
            body: JSON.stringify(formData),
          }
        )
          .then((response) => response.json())
          .then((data) => {
            console.log("response:", data);
            document.getElementById("loader").style.display = "none";
            document.getElementById("buttonText").style.display =
              "inline-block";
            if (data && data?.message == "success") {
              frm.reset();
              document.getElementById("message-success").style.display =
                "block";
              // console.log('doccument', document.getElementById('message-success'))
              setTimeout(function () {
                document.getElementById("message-success").style.display =
                  "none";
              }, 2000);
              // alert('Mail sent successfully.')
            } else {
              alert("Something went wrong!");
            }
          })
          .catch((error) => {
            document.getElementById("loader").style.display = "none";
            document.getElementById("buttonText").style.display =
              "inline-block";
            console.error("Error:", error);
            alert("Something went wrong!");
          });
      }
    </script>
