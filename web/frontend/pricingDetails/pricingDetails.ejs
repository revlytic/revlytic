        height: 20px;
        animation: spin 1s linear infinite;
        display: none;
      }

      /* Add a simple animation for the loader */
      @keyframes spin {
        0% {
          transform: rotate(0deg);
        }
        100% {
          transform: rotate(360deg);
        }
      }
